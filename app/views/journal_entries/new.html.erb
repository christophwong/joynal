<div class="container">
  <h1>Please create a new journal entry:</h1>
  <%= form_for @journal_entry do |f| %>
    <%= f.text_area :content, cols: 80, rows: 20 %><br>
    <%= f.select :emotion_rating, options_for_select((1..5).to_a) %><br>
    <%= f.text_field :tags, value: '' %>
    <%= f.submit %>
  <% end %>




</div>
